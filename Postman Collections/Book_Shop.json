		{
			"name": "Get Books",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "b3ddb491-9b93-4dfc-a17a-6450f8d5903a",
						"exec": [
							"pm.test(\"Status code is 200\", () => {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"const response = pm.response.json();\r",
							"const book = response.filter((book) => book.available === true );\r",
							"console.log(book[0]);\r",
							"pm.globals.set(\"Available Non-Fiction Book ID\", book[0].id);\r",
							"\r",
							"pm.test(\"Check if book is availble\", () => {\r",
							"    pm.expect(book[0]).to.be.an('object');\r",
							"    pm.expect(book[0].available).to.be.true;\r",
							"});\r",
							"\r",
							"pm.test(\"Check if book is Non-Fiction\", () => {\r",
							"    pm.expect(book[0].type === \"non-fiction\").to.be.true;\r",
							"    pm.expect(book[0].type).to.eql(\"non-fiction\")\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
